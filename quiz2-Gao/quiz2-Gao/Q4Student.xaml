<Window x:Class="quiz2_Gao.Q4Student"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:quiz2_Gao"
        mc:Ignorable="d"
        Title="Q4Student" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox x:Name="tbNewStudent" HorizontalAlignment="Center" Width="300" Height="50" Margin="20"></TextBox>
            <Button x:Name="btnAddStudent" Width="100" Margin="20" Click="btnAddStudent_Click">Add Student</Button>
            <ListBox x:Name="lbStudents" Width="300" Height="235" Margin="246,0"/>
        </StackPanel>
    </Grid>
</Window>
